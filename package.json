{
  "name": "currency-utilities",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4000",
    "build": "npm run standardize && ng build --configuration=production",
    "sanity": "npm run lint && ng test --watch false && ng e2e --port 4001 --suite sanity",
    "dev-coverage": "npm run standardize && ng test --code-coverage  --browsers Chrome",
    "dev-e2e": "npm run standardize && ng e2e --devServerTarget=",
    "dev-test": "npm run standardize && ng test --browsers Chrome",
    "lint": "npm run standardize && ng lint  && npm run eslint && npm run cspell",
    "standardize": "prettier --write src/**/*.{ts,js,html,css} && prettier --write e2e/**/*.{ts,js}",
    "e2e": "npm run standardize && ng e2e --port 4001",
    "test": "npm run standardize && ng test --watch false",
    "coverage": "npm run standardize && ng test --code-coverage --watch false  --browsers ChromeHeadless",
    "cspell": "cspell -- --no-summary  src/**/*.{ts,js,html,css} && cspell -- --no-summary e2e/**/*.{ts,js}",
    "eslint": "eslint --cache --fix src/**/*.{ts,js}",
    "test-c": "npm run standardize && ng test --browsers Chrome",
    "test-f": "npm run standardize && ng test --browsers Firefox",
    "test-e": "npm run standardize && ng test --browsers Edge",
    "test-h": "npm run standardize && ng test --browsers ChromeHeadless",
    "ci-e2e": "ng e2e --protractor-config=e2e/protractor-ci.conf.js --devServerTarget=",
    "ci-test": "npm run standardize && ng test --karmaConfig=src/karma-ci.conf.js --watch false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.9",
    "@angular/cdk": "~9.2.4",
    "@angular/common": "^9.1.9",
    "@angular/compiler": "^9.1.9",
    "@angular/core": "^9.1.9",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "^9.1.9",
    "@angular/material": "~9.2.4",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@angular/router": "^9.1.9",
    "core-js": "^2.6.11",
    "rxjs": "^6.5.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.6",
    "@angular/cli": "^9.1.6",
    "@angular/compiler-cli": "~9.1.9",
    "@angular/language-service": "^9.1.9",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "angular-estree-parser": "^2.0.0",
    "codelyzer": "^5.1.2",
    "cspell": "^4.0.63",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-html-parser": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.3",
    "fs-extra": "^8.1.0",
    "husky": "^4.2.5",
    "jasmine-core": "~2.99.1",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.0.9",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.3.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^10.2.1",
    "prettier": "^2.0.5",
    "protractor": "^7.0.0",
    "protractor-html-reporter-2": "^1.0.4",
    "protractor-jasmine2-screenshot-reporter": "^0.5.0",
    "ts-node": "^8.8.1",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,html,css,md}": [
      "cspell -- --no-summary"
    ],
    "*.{js,ts,html,css}": [
      "prettier --write"
    ],
    "*.{js,ts}": [
      "eslint --cache --fix"
    ]
  }
}
