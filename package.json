{
  "name": "currency-utilities",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "standardize": "prettier **/*.ts --single-quote --write",
    "start": "ng serve --port 4000",
    "build": "npm run standardize && ng build",
    "test": "npm run standardize && ng test --watch false",
    "coverage": "npm run standardize && ng test --code-coverage --watch false ",
    "sanity": "npm run standardize && ng test --watch false && ng e2e --port 4001 --suite sanity",
    "lint": "npm run standardize && ng lint",
    "e2e": "npm run standardize && ng e2e --port 4001",
    "dev-e2e": "npm run standardize && ng e2e --devServerTarget=",
    "dev-test": "npm run standardize && ng test --code-coverage --browsers Chrome",
    "ci-e2e": "ng e2e --protractor-config=e2e/protractor-ci.conf.js --devServerTarget="
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.0",
    "@angular/common": "^9.1.0",
    "@angular/compiler": "^9.1.0",
    "@angular/core": "^9.1.0",
    "@angular/forms": "^9.1.0",
    "@angular/platform-browser": "^9.1.0",
    "@angular/platform-browser-dynamic": "^9.1.0",
    "@angular/router": "^9.1.0",
    "core-js": "^2.6.11",
    "rxjs": "^6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.0",
    "@angular/cli": "^9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "^9.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "editorconfig": "^0.15.3",
    "eslint": "^5.16.0",
    "fs-extra": "^8.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.3.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "^1.19.1",
    "protractor": "^5.4.3",
    "protractor-html-reporter-2": "^1.0.4",
    "protractor-jasmine2-screenshot-reporter": "^0.5.0",
    "ts-node": "^8.8.1",
    "tslint": "~6.1.0",
    "typescript": "~3.7.5"
  }
}
